      "source": [
        "STEP 2: Generate Synthetic Sales Data"
      ],
      "metadata": {
        "id": "GxlwkE-R6FB8"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "np.random.seed(42)\n",
        "dates = pd.date_range(start=\"2025-01-01\",periods=30)\n",
        "products = [\"Pen\",\"Notebook\",\"Marker\",\"Eraser\"]\n",
        "data = []\n",
        "\n",
        "for date in dates:\n",
        "   for product in products:\n",
        "       units_sold= np.random.randint(5, 60)\n",
        "       price = {\n",
        "        \"Pen\": 10,\n",
        "        \"Notebook\": 50,\n",
        "        \"Marker\": 30,\n",
        "        \"Eraser\": 5\n",
        "      }[product]\n",
        "       total_sales = units_sold * price\n",
        "       data.append([date,product,units_sold,price,total_sales])\n",
        "\n",
        "df = pd.DataFrame(data,columns=[\"date\",\"product\",\"units_sold\",\"price\",\"total_sales\"])\n",
        "df.head()"
      ],
